#include "nb_iot_task.h"
/*
{
  "power_level": 0,
  "temperature": [0, 0, 0],
  "imu": [0, 0, 0],
  "gnss": [
    [0, 0, 0],
    [0, 0, 0]
  ],
  "errors": 0
}
*/
void nb_iot_task(void *argument)
{
	//setup
	package_data_t* ready_to_send;
	ready_to_send = read_package_data();
	update_package_data();;

	//HAL_UART_Transmit_IT(&huart3, "NB-Iot\n", 7);
	//loop
	while(1)
	{
		osDelay(DELAY_MAX);
	}

}
